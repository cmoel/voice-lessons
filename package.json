{
  "name": "voice-lessons",
  "productName": "Voice Lessons",
  "version": "1.0.0",
  "description": "Electron/React application for tracking voice lessons and students",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cmoel/voice-lessons.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-win": "electron-forge make --platform=win32",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "Christopher Moeller",
    "email": "cmoel@me.com",
    "url": "http://cmoel.com"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "asar": true
      },
      "electronWinstallerConfig": {
        "name": "voice_lessons"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "voicelessons"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.1",
    "electron-devtools-installer": "^2.0.1",
    "electron-log": "^2.2.7",
    "electron-remote": "^1.2.0",
    "electron-storage": "^1.0.7",
    "js-joda": "^1.5.5",
    "photon": "git://github.com/cmoel/photon.git",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.6",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^3.2.0",
    "electron-prebuilt-compile": "1.7.5"
  }
}
